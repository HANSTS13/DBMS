SELECT * FROM Customers ORDER BY customerName ASC;
SELECT * FROM Orders ORDER BY OrderDate ASC;
SELECT * FROM Employees WHERE BirthDate>TO_DATE('1980-12-31','YYYY-MM-DD') ORDER BY BirthDate DESC;
SELECT * FROM Orders O JOIN Customers C ON O.CustomerID = C.CustomerID WHERE C.Country = 'USA' ORDER BY O.OrderDate DESC;
SELECT CustomerID,COUNT(OrderID) AS TotalOrders FROM Orders GROUP BY CustomerID;
SELECT City, COUNT(CustomerID) AS TotalCustomers FROM Customers GROUP BY City;
SELECT CustomerID, COUNT(OrderID) AS TotalOrders
FROM Orders GROUP BY CustomerID HAVING COUNT(OrderID) > 2;
