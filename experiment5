FROM Orders GROUP BY CustomerID HAVING COUNT(OrderID) > 2;
SELECT CustomerID, COUNT(OrderID) AS TotalOrders FROM Orders GROUP BY CustomerID HAVING COUNT(OrderID) > 2;
SELECT City, COUNT(CustomerID) AS TotalCustomers FROM Customers GROUP BY City HAVING COUNT(CustomerID) >= 3;
